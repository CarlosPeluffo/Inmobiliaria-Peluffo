@model Inmobiliaria_Peluffo.Models.Inquilino

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalles</h1>

<div>
    <h4>Inquilino</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ID.
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.id)
        </dd>
        <dt class = "col-sm-2">
            D.N.I.
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.dni)
        </dd>
        <dt class = "col-sm-2">
            Apellido
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.apellido)
        </dd>
        <dt class = "col-sm-2">
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nombre)
        </dd>
        <dt class = "col-sm-2">
            E-Mail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.mail)
        </dd>
        <dt class = "col-sm-2">
            Teléfono
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.telefono)
        </dd>
        <dt class = "col-sm-2">
            Lugar de Trabajo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.lugarDeTrabajo)
        </dd>
        <dt class = "col-sm-2">
            D.N.I. del Garante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.dniGarante)
        </dd>
        <dt class = "col-sm-2">
            Nombre del Garante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nombreGarante)
        </dd>
        <dt class = "col-sm-2">
            Teléfono del Garante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.telefonoGarante)
        </dd>
        <dt class = "col-sm-2">
            E-Mail del Garante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.mailGarante)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id }) |
    <a asp-action="Index">Volver a la lista</a>
</div>
